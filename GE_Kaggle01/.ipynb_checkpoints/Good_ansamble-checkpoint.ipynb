    "ans = pd.DataFrame()\n",
    "ans[\"id\"] = first[\"Id\"]\n",
    "ans[\"DBH\"] = (first[\"dbh\"]+second[\"dbh\"])/2\n",
    "ans.to_csv(\"Good_ansamble.csv\", index=False)"
